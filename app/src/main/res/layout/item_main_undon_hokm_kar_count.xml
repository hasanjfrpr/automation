<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frame_item_undoneHokm_count"
    android:background="?selectableItemBackground"
    android:layout_marginHorizontal="4dp"
    xmlns:tools="http://schemas.android.com/tools">


    <TextView
        android:id="@+id/TV_item_UndonHokmkar_count_name"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:textSize="16sp"
        android:textStyle="bold"
        android:maxLines="1"
        android:gravity="center_vertical"
        android:layout_marginStart="16dp"
        android:paddingVertical="4dp"
        android:ellipsize="end"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="kadoiald daf" />

    <TextView
        android:id="@+id/TV_item_UndonHokmkar_count"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="end"
        android:gravity="start|center_vertical"
        android:paddingEnd="16dp"
        android:textSize="16sp"
        android:textStyle="bold"
        android:paddingVertical="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/TV_item_bazaryabi_main_personal"
        app:layout_constraintEnd_toStartOf="@+id/TV_item_bazaryabi_main_personal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="23362" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_gravity="bottom"
        android:background="?dividerColor"/>


</FrameLayout>